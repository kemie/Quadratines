# Prefix: Languagesystems
# automatic
languagesystem DFLT dflt;

feature aalt {
# automatic
feature ss01;
} aalt;

feature ccmp {
# automatic
lookup ccmp_Other_1 {
	@CombiningTopAccents = [acutecomb dieresiscomb tildecomb];
	sub [i]' @CombiningTopAccents by [idotless];
} ccmp_Other_1;
} ccmp;

feature ss01 {
# automatic
sub Q by Q.ss01;
sub g by g.ss01;
} ss01;

